{
  "meta": {
    "name": "Sentence Surgery — Passive Voice (Ciešamā kārta) — B1 dataset",
    "version": "0.2",
    "created": "2026-02-07",
    "sources_used": [
      "Ciešamā kārta (Week 5 slides)",
      "Salīmēt / uzlīmēt (Week 5 slides)",
      "Valoda — 10. nodaļa (Week 13 slides)"
    ],
    "notes": [
      "Each item is a repair task: target_lv (correct) + broken_lv (one intentional error).",
      "Main focus: tikt + passive participle agreement (-ts/-ta/-ti/-tas), tense, negation, and typical word order.",
      "Word banks include plausible distractors (wrong aux tense/negation or wrong participle ending)."
    ],
    "missing_data_to_start_building_sentence_surgery": [
      {
        "priority": "P0",
        "dataset": "Verb lexicon (passive-participle generator)",
        "why": "To scale beyond hand-written items and generate distractors reliably.",
        "must_include": [
          "infinitive",
          "past passive participle stem / forms (-ts/-ta/-ti/-tas)",
          "valency notes (what cases/objects the verb usually takes)",
          "common prefixes (ie-, uz-, iz-, sa-, at-...) where relevant"
        ]
      },
      {
        "priority": "P0",
        "dataset": "Noun & noun-phrase lexicon",
        "why": "To guarantee correct gender/number agreement + cases for active↔passive pairs.",
        "must_include": [
          "lemma",
          "gender",
          "number patterns",
          "core forms (Nom/Gen/Acc/Dat sg+pl at minimum)",
          "multiword NP templates (e.g., 'Pirmās grāmatas', 'Eksāmena jautājumi')"
        ]
      },
      {
        "priority": "P0",
        "dataset": "Active↔passive aligned pairs",
        "why": "Sentence Surgery becomes more ‘teaching’ when it shows what changed and why.",
        "must_include": [
          "active_lv (SVO)",
          "passive_lv (ciešamā kārta)",
          "mapping (active object → passive subject, verb → participle, tense → aux form)",
          "optional agent phrase patterns (by-context)"
        ]
      },
      {
        "priority": "P1",
        "dataset": "Error-template library",
        "why": "So the game can generate consistent, pedagogically meaningful ‘broken’ sentences.",
        "must_include": [
          "aux tense swap (tiek↔tika↔tiks)",
          "negation swap (tiek↔netiek etc.)",
          "agreement swap (-ts/-ta/-ti/-tas)",
          "missing-token patterns (blank aux/blank participle)",
          "word-order traps (controlled, B1-safe)"
        ]
      },
      {
        "priority": "P1",
        "dataset": "Difficulty & pedagogy tags",
        "why": "Adaptive practice needs labels to choose the right next task.",
        "must_include": [
          "CEFR sub-skill tags (tense, negation, agreement, dative recipient, time phrase)",
          "difficulty score",
          "topic/context tag"
        ]
      },
      {
        "priority": "P2",
        "dataset": "Audio / pronunciation support",
        "why": "Optional, but huge for retention and confidence.",
        "must_include": [
          "TTS-ready text",
          "stress hints (optional)",
          "minimal pairs for endings (-ts/-ta/-ti/-tas)"
        ]
      }
    ]
  },
